{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": true,
        "sourceMap": true,
        "rootDir": "src",
        "outDir": "generated/lib/",
        "experimentalDecorators": true,
        "jsx": "react",
        "strictNullChecks": true,
        "noLib": true,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true
    }
}
